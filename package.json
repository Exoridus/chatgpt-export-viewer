{
  "name": "chatgpt-data-export-viewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prepare": "husky",
    "build": "tsc -p tsconfig.json && vite build",
    "test": "vitest run --coverage",
    "test:run": "vitest run",
    "fixture:test-zip": "tsx scripts/build-test-fixture.ts",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:stylelint": "stylelint --config stylelint.config.ts \"src/**/*.{scss,css}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\" && stylelint --fix --config stylelint.config.ts \"src/**/*.{scss,css}\"",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "import": "tsx scripts/import-server.ts --output ./dist",
    "dataset:import": "tsx scripts/import-dataset.ts --out ./dist",
    "build:cli": "tsx scripts/build-cli.ts",
    "dist": "npm run build && npm run build:cli -- --out ./dist/import-dataset",
    "commit": "gitmoji -c",
    "validate:commit": "commitlint --config commitlint.config.ts",
    "validate:tags": "tsx scripts/validate-tags.ts",
    "release:notes": "bash scripts/generate-release-notes.sh"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "clsx": "^2.1.1",
    "fflate": "^0.8.2",
    "idb": "^8.0.3",
    "lucide-react": "^0.575.0",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.0.2",
    "react-window": "^1.8.11",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/types": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@yao-pkg/pkg": "^6.14.0",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "fake-indexeddb": "^6.2.4",
    "gitmoji-cli": "^9.7.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^28.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.97.3",
    "stylelint": "^17.3.0",
    "stylelint-config-recess-order": "^7.6.1",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "stylelint-order": "^7.0.1",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
